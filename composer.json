{
    "name": "mapado/php-sdk",
    "description": "PHP SDK for Mapado API",
    "require": {
        "guzzlehttp/guzzle": "~5.0",
        "incenteev/composer-parameter-handler": "~2.0"
    },
    "require-dev": {
        "raulfraile/ladybug": "~1.0",
        "atoum/atoum": "~1.0",
        "symfony/yaml": "~2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": { "Mapado\\Sdk\\": "src/" }
    },
    "authors": [
        {
            "name": "Julien Deniau",
            "email": "julien.deniau@gmail.com"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "src/Tests/parameters.yml"
        }
    }
}
